{
    "name": "apiato/documentation-generator-container",
    "description": "An Apiato container which can creates a documentation from API annotations in your source code.",
    "type": "apiato-container",
    "license": "MIT",
    "extra": {
        "apiato": {
            "container": {
                "name": "Documentation"
            }
        },
        "laravel": {
            "providers": [
                "App\\Containers\\Vendor\\Documentation\\Providers\\DocumentGeneratorServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "apiato/core": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/PraiseCharts/apiato-core"
        }
    ],
    "config": {
        "allow-plugins": {
            "apiato/container-installer": true
        }
    }
}
