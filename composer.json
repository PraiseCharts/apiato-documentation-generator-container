{
    "name": "apiato/documentation-generator-container",
    "description": "An Apiato container which can creates a documentation from API annotations in your source code.",
    "type": "apiato-container",
    "license": "MIT",
    "extra": {
        "apiato": {
            "container": {
                "name": "Documentation"
            }
        },
        "laravel": {
            "providers": [
                "Apiato\\Containers\\Documentation\\Providers\\DocumentGeneratorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.21",
        "apiato/core": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Apiato\\Containers\\Documentation\\": ""
        }
    }
}
